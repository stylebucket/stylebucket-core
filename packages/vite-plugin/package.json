{
  "$schema": "https://json.schemastore.org/package",
  "name": "@stylebucket/vite-plugin",
  "version": "0.1.1",
  "type": "module",
  "license": "MIT",
  "description": "Vite plugin for stylebucket",
  "author": {
    "name": "Brett Wood",
    "url": "https://github.com/woodbrettm"
  },
  "bugs": "https://github.com/stylebucket/stylebucket-core/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/stylebucket/stylebucket-core",
    "directory": "packages/vite-plugin"
  },
  "keywords": [
    "css-in-js",
    "css"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=7.0.0"
  },
  "main": "./build/main.js",
  "types": "./build/main.d.ts",
  "exports": {
    ".": {
      "import": "./build/main.js",
      "types": "./build/main.d.ts"
    }
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "defu": "6.1.4",
    "vite": "5.0.11",
    "@stylebucket/compiler": "workspace:*"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "tsup": "8.0.1",
    "vitest": "1.1.3"
  },
  "scripts": {
    "watch": "tsup --watch",
    "build": "tsup",
    "test:unit": "vitest --run",
    "lint:types": "tsc --noEmit"
  }
}
